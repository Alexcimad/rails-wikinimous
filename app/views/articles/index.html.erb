
<div class="banner d-flex justify-content-between sticky-top">
  <%= link_to "Rails-Wikinimous", articles_path, id: "app_name" %>
  <%= link_to "New Article", new_article_path, class:"btn btn-primary"%>
</div>
<div class="container">
  <h1 class="text-center">Articles list</h1>
  <div class="article text-center">
    <% @articles.each do |article| %>
      <%= link_to article.title, article_path(article), class: "title" %> 
      <p><%=article.content%></p>  
    <% end %>
  </div>
</div>